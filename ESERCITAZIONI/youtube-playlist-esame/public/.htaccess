<IfModule mod_rewrite.c>
    RewriteEngine On

    DirectoryIndex index.php

    RewriteRule ^\. - [F]

    Options -Indexes

    RewriteCond %{REQUEST_URI} ^/youtube-playlist/?$ [NC]
    RewriteRule ^$ index.php [L]

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . index.php [L,QSA]
</IfModule>

